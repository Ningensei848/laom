{
  "name": "laom",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "emit": "swc src/plugins -d src --config-file src/plugins/.swcrc",
    "docusaurus": "docusaurus",
    "dev": "yarn start",
    "start": "yarn emit && docusaurus start",
    "build": "yarn emit && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "typecheck": "tsc",
    "lint": "eslint --cache --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix && yarn format",
    "format": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json,css,md,mdx}'"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/core": "latest",
    "@docusaurus/preset-classic": "latest",
    "@mdx-js/react": "^1.6.21",
    "@remark-embedder/core": "^2.0.0",
    "@remark-embedder/transformer-oembed": "^2.0.0",
    "clsx": "^1.1.1",
    "dotenv": "^16.0.0",
    "next-share": "^0.13.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "latest",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.145",
    "@tsconfig/docusaurus": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "embeddedLanguageFormatting": "off"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    ".docusaurus",
    ".eslintrc.js",
    "sidebars.js",
    "*.config.js"
  ],
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
